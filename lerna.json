{
  "version": "independent",
  "npmClient": "npm",
  "command": {
    "init": {
      "exact": true
    },
    "version": {
      "allowBranch": "main",
      "conventionalCommits": true,
      "exact": true,
      "message": "chore(release): publish"
    },
    "publish": {
      "graphType": "all"
    }
  },
  "ignoreChanges": [
    "*.md"
  ]
}
